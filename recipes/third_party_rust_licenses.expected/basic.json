[
  {
    "cmd": [],
    "name": "ensure_jiri"
  },
  {
    "cmd": [
      "cipd",
      "ensure",
      "-root",
      "[START_DIR]/cipd/jiri",
      "-ensure-file",
      "fuchsia/tools/jiri/linux-amd64 stable",
      "-json-output",
      "/path/to/tmp/json"
    ],
    "infra_step": true,
    "name": "ensure_jiri.ensure_installed",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": [@@@",
      "@@@STEP_LOG_LINE@json.output@    {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"instance_id\": \"resolved-instance_id-of-stable----------\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"package\": \"fuchsia/tools/jiri/linux-amd64\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  ]@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "[START_DIR]/cipd/jiri/jiri",
      "init",
      "-vv",
      "-time",
      "-analytics-opt=false",
      "-rewrite-sso-to-https=true",
      "-j=10",
      "-cache",
      "[CACHE]/git",
      "-shared"
    ],
    "infra_step": true,
    "name": "jiri init"
  },
  {
    "cmd": [
      "[START_DIR]/cipd/jiri/jiri",
      "import",
      "-vv",
      "-time",
      "third_party_rust_crates",
      "https://fuchsia.googlesource.com/manifest"
    ],
    "infra_step": true,
    "name": "jiri import"
  },
  {
    "cmd": [
      "[START_DIR]/cipd/jiri/jiri",
      "update",
      "-vv",
      "-time",
      "-autoupdate=false",
      "-attempts=3"
    ],
    "infra_step": true,
    "name": "jiri update"
  },
  {
    "cmd": [
      "[START_DIR]/scripts/rust/check_rust_licenses.py",
      "--verify",
      "--directory",
      "[START_DIR]/third_party/rust-crates/rustc_deps/vendor"
    ],
    "name": "verify licenses"
  },
  {
    "jsonResult": null,
    "name": "$result"
  }
]