    "infra_step": true,
    "name": "jiri project",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@[@@@",
      "@@@STEP_LOG_LINE@json.output@  {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"branches\": [@@@",
      "@@@STEP_LOG_LINE@json.output@      \"(HEAD detached at c22471f)\"@@@",
      "@@@STEP_LOG_LINE@json.output@    ], @@@",
      "@@@STEP_LOG_LINE@json.output@    \"current_branch\": \"\", @@@",
